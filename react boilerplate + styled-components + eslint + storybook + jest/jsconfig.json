{
    "compilerOptions": {
        "jsx": "react",
        "target": "ES6",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./src",
        "paths": {
            "*": [
                "*"
            ]
        },
        "module": "commonjs"
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ],
    "include": [
      "src/**/*.jsx",
      "src/**/*.js"
    ]
}
